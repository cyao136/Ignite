<div class="page-header"><h1>Upload Demo</h1></div>

<%= form_for :project, :url => {:controller => 'projects', :action => 'update', :id => project_id} do |f| %>

	<%= f.fields_for :demos_attributes, :html => {:multipart => true} do |d| %>
		<div class="form-group">
			<%= d.label :name %>
			<%= d.text_field :name, class: 'form-control' %>
		</div>
		
		<div class="form-group">
			<%= d.label :version %>
			<%= d.text_field :version, class: 'form-control' %>
		</div>

		<div class="form-group">
			<%= d.label :asset %>
			<%= d.file_field :asset, class: 'form-control'%>
		</div>
	<% end%>

	<%= f.submit 'Upload', class: 'btn btn-primary' %>
<% end %>