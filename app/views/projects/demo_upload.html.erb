<div class="page-header"><h1>Upload Demo</h1></div>

<%= form_for :demo, :url => {:controller => 'projects', :action => 'upload_demo', :id => project_id} do |f| %>

	<div class="form-group">
		<%= f.label :name %>
		<%= f.text_field :name, class: 'form-control' %>
	</div>
	
	<div class="form-group">
		<%= f.label :version %>
		<%= f.text_field :version, class: 'form-control' %>
	</div>

	<div class="form-group">
		<%= f.label :asset %>
		<%= f.file_field :asset, class: 'form-control'%>
	</div>

	<%= f.submit 'Upload', class: 'btn btn-primary' %>
<% end %>