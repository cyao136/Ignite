<% provide(:title, "New Project") %>
<div class="container">
	<h1>Create a project</h1>
	<%= form_for(:project, :url => {:controller => 'projects', :action => 'create'}) do |f| %>

		<%= f.label :name %>
		<%= f.text_field :name, class: 'form-control' %>

		<%= f.submit "No Demo", class: 'form-control' %>
		<%= f.submit "Has Demo", :name => 'demo', class: 'form-control' %>

	<% end %>
</div>
